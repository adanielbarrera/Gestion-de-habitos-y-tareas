<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>RegistroServlet</servlet-name>
        <servlet-class>servlet.RegistroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TareasServlet</servlet-name>
        <servlet-class>servlet.TareasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HabitosServlet</servlet-name>
        <servlet-class>servlet.HabitosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardServlet</servlet-name>
        <servlet-class>servlet.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>servlet.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompletarTareaServlet</servlet-name>
        <servlet-class>servlet.CompletarTareaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarTareaServlet</servlet-name>
        <servlet-class>servlet.EliminarTareaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompletarHabitoServlet</servlet-name>
        <servlet-class>servlet.CompletarHabitoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarHabitoServlet</servlet-name>
        <servlet-class>servlet.EliminarHabitoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistroServlet</servlet-name>
        <url-pattern>/RegistroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TareasServlet</servlet-name>
        <url-pattern>/TareasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HabitosServlet</servlet-name>
        <url-pattern>/HabitosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashboardServlet</servlet-name>
        <url-pattern>/DashboardServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/TestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CompletarTareaServlet</servlet-name>
        <url-pattern>/CompletarTareaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarTareaServlet</servlet-name>
        <url-pattern>/EliminarTareaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CompletarHabitoServlet</servlet-name>
        <url-pattern>/CompletarHabitoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarHabitoServlet</servlet-name>
        <url-pattern>/EliminarHabitoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
